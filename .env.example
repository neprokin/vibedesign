# Figma API
FIGMA_API_KEY=your_figma_api_key
FIGMA_FILE_KEY=your_figma_file_key

# Figma OAuth (optional)
FIGMA_OAUTH_CLIENT_ID=your_figma_oauth_client_id
FIGMA_OAUTH_CLIENT_SECRET=your_figma_oauth_client_secret
FIGMA_OAUTH_REDIRECT_URI=http://localhost:3333/oauth/callback

# Figma API Caching
FIGMA_API_CACHE_TTL=300 # Time to live in seconds (default: 5 minutes)

# OpenAI API
OPENAI_API_KEY=your_openai_api_key

# Server Configuration
PORT=3333
MODE=http # http or stdio
VERBOSE=false

# WebSocket Configuration
WEBSOCKET_URL=ws://localhost:8765

# Logging
LOG_LEVEL=INFO # DEBUG, INFO, WARN, ERROR
LOG_FILE=logs/mcp_server.log 